%prefix onto <http://psi.metaleaks.org/cablegate/ontology/>
%prefix dc <http://purl.org/dc/elements/1.1/>
%prefix geo <http://www.w3.org/2003/01/geo/wgs84_pos#>

onto:is-partial
  - "is partial".

onto:partial
  - "partial".

onto:reference-id
  - "reference identifier".

onto:transmission-id
  - "transmission identifier".

onto:tag
  - "TAG".

onto:geo-tag ako onto:tag;
  - "Geo TAG".

onto:country-tag ako onto:geo-tag;
  - "country TAG".

onto:region-tag ako onto:geo-tag;
  - "region TAG".

onto:classification-level 
  - "classification level".

onto:diplomatic-cable
  - "diplomatic cable".

onto:subject
  - "subject".

onto:summary
  - "summary".

dc:title
  - "title".

geo:lat - "latitude".
geo:long - "longitude".
